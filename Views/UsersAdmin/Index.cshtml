@model IEnumerable<IdentitySample.Models.ApplicationUser>

@{
    ViewBag.Title = "Accounting Management";
}

<h2>Accounting Management</h2>

<p>
    @Html.ActionLink("New user", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Username
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("OK to delete new", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
